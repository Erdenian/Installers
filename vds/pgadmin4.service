[Unit]
Description=Pgadmin4 Service
After=network.target

[Service]
User=pgadmin
Group=pgadmin
WorkingDirectory=/opt/pgadmin4/
Environment="PATH=/opt/pgadmin4/bin"
ExecStart=/opt/pgadmin4/bin/python3 /opt/pgadmin4/lib/python3.6/site-packages/pgadmin4/pgAdmin4.py
PrivateTmp=true

[Install]
WantedBy=multi-user.target